{
	"manifest_version": 2,

	"name": "Open AliExpress Invoice Generator",
	"description": "This interface lets the user generate invoices from the order made on AliExpress",
	"version": "0.18.1.3",	

	"browser_action": {
		"default_icon": "icon.png"
	},
	"content_scripts": [
		{
			"matches": ["*://trade.aliexpress.com/*"],
			"css": ["orders.css"],
			"js": ["orders.js"],
			"run_at": "document_end"
		}
	],
	"permissions": [
		"activeTab",
		"storage"
  ]
}
